{
  "$schema": "https://swc.rs/schema.json",
  "minify": true,
  "sourceMaps": true,
  "env": {
    "target": "node",
    "mode": "entry",
    "coreJs": "3"
  },
  "module": {
    "type": "commonjs"
  },
  "jsc": {
    "externalHelpers": true,
    "minify": {
      "format": {
        "comments": "all"
      }
    },
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": false,
      "dynamicImport": false
    }
  }
}
